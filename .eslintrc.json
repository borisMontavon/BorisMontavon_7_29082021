{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "quotes": "off",
        "indent": "off",
        "no-trailing-spaces": "off",
        "no-param-reassign": "off",
        "no-tabs": "off",
        "comma-dangle": ["error", "never"],
        "no-new": "off",
        "max-len": ["error", { "comments": 150 }, { "code": 150 }],
        "import/no-cycle": "off",
        "no-underscore-dangle": "off",
        "no-bitwise": "off",
        "no-mixed-operators": "off",
        "implicit-arrow-linebreak": "off"
    }
}